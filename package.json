{
	"name": "obsidian-todos-api",
	"version": "1.3.2",
	"description": "Obsidian plugin to expose todos via REST API",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "npm version patch && git commit --amend --no-edit && node version-bump.mjs && git add package.json manifest.json versions.json && git commit --amend --no-edit",
		"test": "echo \"No tests specified\" && exit 0"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"todos",
		"tasks",
		"rest-api",
		"dataview"
	],
	"author": "mtuckerb",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"obsidian-local-rest-api": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"moment": "^2.30.1"
	}
}
